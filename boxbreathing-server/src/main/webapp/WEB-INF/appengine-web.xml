<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <service>boxbreathing</service>
    <runtime>java8</runtime>
    <threadsafe>true</threadsafe>

    <static-files>
        <include path="/**.html" />
        <include path="/**.htm" />
        <include path="/**.css" />
        <include path="/**.js" />
        <include path="/**.json" />

        <include path="/**.png" />
        <include path="/**.jpg" />
        <include path="/**.jpeg" />
        <include path="/**.gif" />
        <include path="/**.bmp" expiration="1d" />

        <include path="/**.ico" expiration="1d" />

        <include path="/**.ttf" expiration="1d" />
        <include path="/**.woff" expiration="1d" />
        <include path="/**.woff2" expiration="1d" />
        <include path="/**.eot" expiration="1d" />

        <include path="/**.pmd" expiration="1d" />
        <include path="/**.pmx" expiration="1d" />
        <include path="/**.vmd" expiration="1d" />
        <include path="/**.x" expiration="1d" />

    </static-files>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>
</appengine-web-app>
