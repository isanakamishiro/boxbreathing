
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="urn:import:gwt.material.design.client.ui https://isanakamishiro.github.io/gmdschemas/2.0/gwt-material-ui.xsd">
    <!--xsi:schemaLocation="urn:import:gwt.material.design.client.ui gwt-material-ui.xsd">-->

    <m:MaterialPanel ui:field="rootPanel" height="100%">
        <m:MaterialHeader>
            <!--<m:MaterialNavBar ui:field="navBar" activates="sidenav" backgroundColor="GREY_LIGHTEN_1" shadow="0" width="64px">-->
            <m:MaterialNavBar ui:field="navBar" activates="sidenav" shadow="0">
                <m:MaterialNavBrand ui:field="navBarBrand" fontSize="1.2em" text=""/>
            </m:MaterialNavBar>
        </m:MaterialHeader>

        <m:MaterialSideNavPush m:id="sidenav" width="220" closeOnClick="true" showOnAttach="false">
            <m:MaterialLink text="Circle" iconType="ALL_OUT" href="#circle"/>
            <m:MaterialLink text="Box" iconType="CHECK_BOX_OUTLINE_BLANK" href="#box"/>
            <m:MaterialLink text="Kizuna" iconType="ACCESSIBILITY" href="#kizuna"/>
            <!--<m:MaterialLink text="Box" iconType="CHECK_BOX_OUTLINE_BLANK" href="#box"/>-->
        </m:MaterialSideNavPush>

        <m:MaterialContainer ui:field="container" paddingTop="64" paddingBottom="12" height="100%"/>

        <m:MaterialFAB>
            <m:MaterialButton ui:field="btnAbout" iconType="LIVE_HELP" waves="DEFAULT"
                              type="FLOATING" size="LARGE" backgroundColor="BLUE" href="http://quietkit.com/box-breathing/"/>
        </m:MaterialFAB>

        <!--<m:MaterialFooter />-->
    </m:MaterialPanel>
</ui:UiBinder>
